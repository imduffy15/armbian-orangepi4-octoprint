<?xml version="1.0" standalone='no'?>
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
 <name replace-wildcards="yes">OctoPrint on %h</name>
 <service>
   <type>_http._tcp</type>
   <port>80</port>
   <host-name>octoprint.local</host-name>
   <txt-record>path=/</txt-record>
   <txt-record>description=3D Printer Management Interface</txt-record>
   <txt-record>service=octoprint</txt-record>
 </service>
</service-group>